 => { \r\n    const [email, setEmail]          = useState(\"\") \r\n    const [password, setPassword]    = useState(\"\")       \r\n\r\n    const handleLogin = (e) => {  \r\n        e.preventDefault();\r\n        const logUser=\r\n        {\r\n            \"username\": email,\r\n            \"email\": email,\r\n            \"password\": password\r\n          }\r\n          console.log(\"log in button\")  \r\n        axios\r\n          .post('http://localhost:8000/users/api/login/', logUser)\r\n          .then((res) => {\r\n            \r\n               if(res.data.user != null){\r\n                 \r\n                  if(res.data.user.is_charity){ \r\n                    console.log(\"is charity:\", res.data.user.is_charity)\r\n                    Storage.set(\"token\", res.data.access_token)\r\n                    console.log(\"token:\", Storage.get(\"token\"))\r\n                    navigate(`/charity-dashboard`)  \r\n\r\n \r\n                  }else{\r\n                    console.log(\"is client:\", res.data.user.is_charity == false)\r\n                    navigate(`/user-dashboard`)  \r\n                  }\r\n               }\r\n            } )  \r\n        }\r\n    return (\r\n        <>\r\n        <nav id=\"navbar-transparent\" className=\"navbar\">\r\n            <Link className =\"navbar-brand  pl-5\" to=\"/\" ><img src={`${Home}`} className=\"img-logo\"/></Link>\r\n        </nav>\r\n        <div className=\"container register\">\r\n            <div className=\"row\">\r\n                <div className=\"col-md-3 register-left\">\r\n                    <img src={`${HomeLogo}`} alt=\"logo\"/>\r\n                    <h3>Welcome</h3>\r\n                    <p>Thank you for joining us </p>\r\n                </div>\r\n                <div className=\"col-md-9 register-right\">\r\n                    <h3 className=\"register-heading\">Login</h3>\r\n                    <div className=\"row register-form \">\r\n                        <div className=\"col-md-12\">\r\n                            <div className=\"form-group col-md-6 mx-auto\">\r\n                                <input type=\"email\" className=\"form-control\" placeholder=\"Email Address*\" onChange={(e) => setEmail(e.target.value)} value={email}/>\r\n                            </div>\r\n                            <div className=\"form-group col-md-6 mx-auto\">\r\n                                <input type=\"password\" className=\"form-control\" placeholder=\"Password *\" onChange={(e) => setPassword(e.target.value)} value={password}/>\r\n                            </div>\r\n                            <div className=\"form-group col-md-6 mx-auto\">\r\n                                <input type=\"submit\" className=\"btnRegister\" value=\"Login\" onClick={handleLogin}/>\r\n                            </div>\r\n                        </div>\r\n                    </div> \r\n                    <p className=\"have-account\">Not a member? <Link to=\"/register\"><span>Sign up</span></Link></p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        </>\r\n    )\r\n};\r\n\r\nexport default Login;\r\n// commit\r\n// 092d  9e00 ","C:\\Users\\great\\OneDrive\\Desktop\\6032021charity\\charityVersion11-01-2020\\client\\src\\components\\Register.jsx",["132","133"],"import React, {useState} from 'react';\r\nimport UserRegister from './UserRegister';\r\nimport CharityRegister from './CharityRegister';\r\nimport { Link } from '@reach/router';\r\nimport HomeLogo from '../images/home.png';\r\nimport Home from '../images/house.png';\r\n\r\n\r\nconst Register = () => {\r\n    const [currentUserState, setCurrentUserState]  = useState(true)\r\n    const [currentCharityState, setCurrentCharityState]  = useState(false)\r\n\r\n    const toggleClass=()=>{\r\n        setCurrentUserState(!currentUserState);\r\n        setCurrentCharityState(!currentCharityState);\r\n    };\r\n\r\n    return (\r\n        <>\r\n        <nav id=\"navbar-transparent\" className=\"navbar\">\r\n            <Link className =\"navbar-brand  pl-5\" to=\"/\" ><img src={`${Home}`} className=\"img-logo\"/></Link>\r\n        </nav>\r\n        <div className=\"container register\">\r\n            <div className=\"row\">\r\n                <div className=\"col-md-3 register-left\">\r\n                    <img src={`${HomeLogo}`} alt=\"logo\"/>\r\n                    <h3>Welcome</h3>\r\n                    <p>Thank you for joining us </p>\r\n                </div>\r\n                <div className=\"col-md-9 register-right\">\r\n                    <ul className=\"nav nav-tabs nav-justified\">\r\n                        <li className=\"nav-item\">\r\n                            <Link to=\"/register\" className={`nav-link ${currentUserState ? 'active': ''}`} onClick={toggleClass}>User</Link>\r\n                        </li>\r\n                        <li className=\"nav-item\">\r\n                            <Link to=\"/register\" className={`nav-link ${currentCharityState ? 'active': ''}`} onClick={toggleClass}>Charity</Link>\r\n                        </li>\r\n                    </ul>\r\n                    <div>\r\n                        {currentUserState==true?<UserRegister/>:<CharityRegister/>}\r\n                        <p className=\"have-account\">Already have an account? <Link to=\"/login\"><span>Sign in</span></Link></p>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        </>\r\n    )\r\n};\r\n\r\nexport default Register;","C:\\Users\\great\\OneDrive\\Desktop\\6032021charity\\charityVersion11-01-2020\\client\\src\\components\\UserPage.jsx",["134","135","136","137"],"import React, {useState, useEffect} from 'react';\r\nimport {Link } from '@reach/router';\r\nimport Man from '../images/man.png';\r\nimport Woman from '../images/woman.png';\r\nimport Home from '../images/house.png';\r\nimport EditUser from './EditUser';\r\nimport EditUserLocation from './EditUserLocation';\r\nimport UserAc